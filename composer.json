{
    "name": "makinacorpus/apubsub",
    "type" : "library",
    "description": "Provides an asynchronous PubSub like API with Drupal implementations",
    "homepage" : "http://github.com/makinacorpus/apubsub",
    "license" : "GPL-2",
    "authors": [
        {
            "name": "Pierre Rineau",
            "email": "pierre.rineau@makina-corpus.com"
        },
        {
            "name": "Makina Corpus",
            "homepage": "http://makina-corpus.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "MakinaCorpus\\APubSub\\": "src/"
        },
        "exclude-from-classmap": [
            "/src/Tests/"
        ]
    },
    "suggest" : {
        "symfony/dependency-injection" : "For notification formatters registration",
        "symfony/event-dispatcher" : "Allows notification to be tied to event subscribers"
    },
    "minimum-stability" : "dev"
}